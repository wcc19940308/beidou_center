package com.ctbt.beidou.base.bo;

public class ResultView {
	
	private String flag;
	private String msg;
	private Object data;
	
	public ResultView(String flag) {
		this.flag = flag;
	}

	public ResultView(String flag, String msg) {
		this.flag = flag;
		this.msg = msg;
	}

	public ResultView(String flag, String msg, Object data) {
		this.flag = flag;
		this.msg = msg;
		this.data = data;
	}
	
	public String getFlag() {
		return flag;
	}
	public void setFlag(String flag) {
		this.flag = flag;
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
	public Object getData() {
		return data;
	}
	public void setData(Object data) {
		this.data = data;
	}

	
}
