.ctbt-btn {
	height: 23px;
	overflow: hidden;
	width: 70px;
	line-height: 23px;
	cursor: pointer;
	position: relative;
	text-align:center;
	border:1px solid #D3D3D3;
	color:#333333;
	background:url('../images/ui/button.gif') repeat-x center center;
}

.ctbt-btn-over, .ctbt-btn:hover {
	background-image:url('../images/ui/button-over.gif'); border-color:#D3D3D3;
}
.ctbt-btn-disabled, .ctbt-btn:disabled {
	background-image:url('../images/ui/button-disabled.gif'); color:#9D9D9E;
}

.ctbt-btn-popselect {
	height: 23px;
	width: 23px;
	overflow: hidden;
	line-height: 23px;
	cursor: pointer;
	position: relative;
	text-align:center;
	border:0px;
	color:#333333;
	background:url('../images/icons/popselect.png') center center;
}

/*ctbt_query_from_input*/
.ctbt_qf_input{
	width: 95%;
}

.ctbt_qf_select{
	width: 95%;
}

.showUserDiv{
	width: 95%;
	height: 80px;
	border: 1px solid #333333;
	float: left;
	overflow: auto;
}

.userDivBlock{
	width: 100px;
	height: 20px;
	border: 1px solid #333333;
	float: left;
	margin:2px;
	cursor: pointer;
}

.userDivBlock2{
	width: 100px;
	height: 20px;
	border: 2px solid #CC0000;
	float: left;
	margin:3px;
	cursor: pointer;
}

.userDivBlockClose{
	width: 11px;
	height: 11px;
	border: 1px solid #333333;
	float: right;
	cursor: pointer;
	background:url('../js/ligerUI/skins/icons/icon-close-over.gif') center center;
}
